		"Linked_service_1_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "devbstoragejsrsecret"
		},
		"LnkAzureKeyVault1_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://my-first-key-vault-dvjsr.vault.azure.net/"
		},
		"Linked_service_2_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "conectionstringoracle"
		},
		"HttpServer1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "rsrefeewfwefwefwef"
